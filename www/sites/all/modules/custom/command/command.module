<?php

function command_init() {
//    drupal_add_js('misc/jquery.form.js');
    drupal_add_library('system', 'jquery.form');
    drupal_add_library('system', 'drupal.ajax');
}

function command_menu() {
    $items = array();

    $items['command/prompt'] = array(
        'title' => t('Command prompt'),
        'page callback' => 'command_prompt',
        'access arguments' => array('access content'),
        'type' => MENU_CALLBACK,
        'file' => 'command.inc'
    );

    $items['command/prompt/show'] = array(
        'title' => t('Show command prompt'),
        'page callback' => 'command_prompt_show',
        'access arguments' => array('access content'),
        'type' => MENU_NORMAL_ITEM,
        'file' => 'command.inc'
    );

    return $items;
}