/**
 * Implements hook_menu()
 */

function erpal_dav_menu() {
	
	
	$items['erpal_dav'] = array(
		'page callback' => 'erpal_dav_init_server',
		'title' => 'ERPAL DAV',
    	'description' =>  'ERPAlDAV Settings',
    	'file' => 'include/erpal_dav_server.php',
    	'type' => MENU_CALLBACK,
    	'access callback' => true
	
	);
	
	$items['admin/erpal/erpal_dav/test'] = array(
		'page callback' => '_test',
		'title' => 'ERPAL DAV',
    	'description' =>  'ERPAlDAV Test page',
    	'file' => 'include/erpal_dav.admin.inc',
    	'access callback' => true
	
	);
	
	$items['admin/erpal/erpal_dav'] = array(
		'page callback' => 'drupal_get_form',
		'page arguments' => array('erpal_dav_admin_form'),
		'title' => 'ERPAL DAV',
    	'description' =>  'ERPAlDAV Settings',
    	'file' => 'include/erpal_dav.admin.inc',
    	'access callback' => true
	
	);
	
	return $items;
		
}

